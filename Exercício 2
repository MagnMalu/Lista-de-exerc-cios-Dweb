<body>
    <form name="formulario">
        NÃºmero:<input type="text" name="numero"><br/>


        <input type="button" value="Tabuada"
        onclick="Tabuada()">
    </form>
    <div id="resultado"></div>

    <script src="Tabuada.js"></script>
</body>
