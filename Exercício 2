Tabuada.js
function Tabuada(){
    var numero=parseInt(document.formulario.numero.value);
    var resultado="";
    for(var i=1;i<=10;i++){
        resultado+=numero+"x"+i+"="+(numero*i)+"<br>";
    }
    document.getElementById("resultado").innerHTML = resultado;
}

